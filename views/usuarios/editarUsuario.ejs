    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Editar</title>
    <link rel="stylesheet" type="text/css" href="/style2.css">
</head>
<body>
    <!--No tiene que funcionar-->
    <h1>Aquí puedes editar la reseva <%=usuario.id%></h1>
    <form action="/editar/:id" method="PUT">
        <label>Correo:</label>
        <input type="text" name="correo" value="<%= usuario.correo %>" required><br>
        <label>Contraseña:</label>
        <input type="text" name="contraseña" value="<%= usuario.contrasenya %>" required><br>
